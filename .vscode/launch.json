    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:8000",
            "webRoot": "${workspaceFolder}/mod3/templates"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "env": {
                "DJANGO_SECRET_KEY":"!xy&!4dmi1!cbusz$%&uay$bkf9$)bbqx&&t=#bru^@46+hk",
                "DJANGO_SETTINGS_MODULE":"config.settings.local",
            },
            "django": true
        },
        {
            "name": "Python: Django Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "test", "--verbosity=2"
            ],
            "env": {
                "DJANGO_SECRET_KEY":"!xy&!4dmi1!cbusz$%&uay$bkf9$)bbqx&&t=#bru^@46+hk",
                "DJANGO_SETTINGS_MODULE":"config.settings.local",
            },
